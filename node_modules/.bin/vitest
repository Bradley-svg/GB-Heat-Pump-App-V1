#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/Work/GREENBRO/GB-Heat-Pump-App-V1/node_modules/.pnpm/vitest@4.0.8_@types+node@24_77da19439d67bac6b41d07a2f150a02e/node_modules/vitest/node_modules:/mnt/d/Work/GREENBRO/GB-Heat-Pump-App-V1/node_modules/.pnpm/vitest@4.0.8_@types+node@24_77da19439d67bac6b41d07a2f150a02e/node_modules:/mnt/d/Work/GREENBRO/GB-Heat-Pump-App-V1/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/Work/GREENBRO/GB-Heat-Pump-App-V1/node_modules/.pnpm/vitest@4.0.8_@types+node@24_77da19439d67bac6b41d07a2f150a02e/node_modules/vitest/node_modules:/mnt/d/Work/GREENBRO/GB-Heat-Pump-App-V1/node_modules/.pnpm/vitest@4.0.8_@types+node@24_77da19439d67bac6b41d07a2f150a02e/node_modules:/mnt/d/Work/GREENBRO/GB-Heat-Pump-App-V1/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../vitest/vitest.mjs" "$@"
fi
