{
  "name": "@greenbro/mobile",
  "version": "0.1.0",
  "private": true,
  "main": "expo/AppEntry",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "lint": "cross-env ESLINT_USE_FLAT_CONFIG=false eslint src --ext .ts,.tsx",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "jest --watchAll=false --coverage",
    "e2e:build:ios": "detox build --config detox.config.js --configuration ios.sim.debug",
    "e2e:test:ios": "detox test --config detox.config.js --configuration ios.sim.debug",
    "e2e:build:android": "detox build --config detox.config.js --configuration android.emu.debug",
    "e2e:test:android": "detox test --config detox.config.js --configuration android.emu.debug"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.0",
    "@react-native-community/netinfo": "^11.3.3",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/native": "^6.1.17",
    "expo": "~52.0.16",
    "expo-constants": "~16.0.1",
    "expo-dev-client": "~5.0.20",
    "expo-haptics": "~14.0.1",
    "expo-linking": "~7.0.5",
    "expo-modules-core": "~1.12.26",
    "expo-secure-store": "~14.0.1",
    "expo-status-bar": "~2.0.0",
    "react": "18.2.0",
    "react-native": "0.75.4",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-svg": "15.8.0"
  },
  "devDependencies": {
    "@types/expo__vector-icons": "^10.0.2",
    "@babel/core": "^7.25.2",
    "@react-native-community/cli": "latest",
    "@config-plugins/detox": "^9.0.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.2.64",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "babel-jest": "^29.7.0",
    "babel-preset-expo": "^12.0.0",
    "cross-env": "^7.0.3",
    "detox": "^20.45.1",
    "eslint": "^8.57.1",
    "eslint-config-universe": "^12.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-native": "^4.0.0",
    "jest": "^29.7.0",
    "jest-expo": "^52.0.0",
    "prettier": "^3.3.3",
    "react-test-renderer": "18.2.0",
    "typescript": "^5.9.3",
    "uri-scheme": "^1.4.9"
  }
}
