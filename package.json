{
  "name": "greenbro-worker",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "wrangler dev --remote",
    "deploy": "wrangler deploy",
    "postinstall": "node ./scripts/install-frontend.mjs",
    "prepare": "husky install",
    "frontend:install": "node ./scripts/install-frontend.mjs",
    "install-frontend:test": "node ./scripts/test-install-frontend.mjs",
    "frontend:dev": "npm --prefix frontend run dev",
    "frontend:watch": "npm --prefix frontend run dev -- --host 0.0.0.0",
    "frontend:build": "npm --prefix frontend run build",
    "frontend:preview": "npm --prefix frontend run preview",
    "frontend:lint": "npm --prefix frontend run lint",
    "frontend:test": "npm --prefix frontend run test",
    "frontend:test:a11y": "npm --prefix frontend run test:a11y",
    "frontend:test:coverage": "npm --prefix frontend run test:coverage",
    "mobile:install": "npm --prefix mobile install --no-audit --progress=false",
    "mobile:start": "npm --prefix mobile start",
    "mobile:android": "npm --prefix mobile run android",
    "mobile:ios": "npm --prefix mobile run ios",
    "mobile:test": "npm --prefix mobile run test",
    "mobile:lint": "npm --prefix mobile run lint",
    "migrate:apply": "wrangler d1 migrations apply GREENBRO_DB",
    "migrate:apply:local": "wrangler d1 migrations apply GREENBRO_DB --local",
    "migrate:list": "wrangler d1 migrations list GREENBRO_DB",
    "seed:dev": "wrangler d1 execute GREENBRO_DB --local --file ./seeds/dev/seed.sql",
    "cron:deploy": "wrangler triggers deploy",
    "typecheck": "tsc --noEmit",
    "build": "npm run frontend:build && wrangler deploy --dry-run --outdir ./dist",
    "lint": "npm run lint:worker && npm run frontend:lint",
    "lint:worker": "eslint --max-warnings=0 src scripts",
    "test": "cross-env ROLLUP_SKIP_NODE_RESOLUTION=1 node ./node_modules/vitest/vitest.mjs run src",
    "publish:r2": "node ./scripts/publish-r2-assets.mjs",
    "ops:r2:bootstrap": "node ./scripts/bootstrap-app-static.mjs",
    "test:integration": "cross-env ROLLUP_SKIP_NODE_RESOLUTION=1 node ./node_modules/vitest/vitest.mjs run tests/integration",
    "test:smoke": "cross-env ROLLUP_SKIP_NODE_RESOLUTION=1 node ./node_modules/vitest/vitest.mjs run tests/smoke",
    "test:security": "cross-env ROLLUP_SKIP_NODE_RESOLUTION=1 node ./node_modules/vitest/vitest.mjs run tests/security",
    "smoke": "npm run test:smoke",
    "security": "npm run test:security",
    "test:ci": "cross-env ROLLUP_SKIP_NODE_RESOLUTION=1 node ./node_modules/vitest/vitest.mjs run src tests/integration tests/smoke tests/security --coverage --passWithNoTests",
    "check:prod-shim": "node ./scripts/ensure-prod-shim-disabled.mjs --env production",
    "ops:db:sanity": "node ./scripts/db-sanity-check.mjs",
    "guard:forbidden": "node .github/scripts/forbidden-fields-lint.js",
    "guard:pii": "node .github/scripts/pii-regex-scan.js",
    "guard:all": "npm-run-all -s guard:forbidden guard:pii"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.36.0",
    "@miniflare/d1": "^2.14.4",
    "@types/react-native": "^0.72.8",
    "@vitest/coverage-v8": "^4.0.7",
    "better-sqlite3": "^12.4.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.38.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^6.1.1",
    "fast-glob": "^3.3.3",
    "globals": "^15.11.0",
    "husky": "^9.1.7",
    "jsdom": "^27.1.0",
    "lint-staged": "^16.2.6",
    "minimatch": "^10.1.1",
    "npm-run-all": "^4.1.5",
    "rollup": "4.52.5",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2",
    "vitest": "^4.0.7",
    "wrangler": "^4.45.4"
  },
  "dependencies": {
    "itty-router": "^5.0.1",
    "jose": "^5.9.3",
    "zod": "^3.23.8"
  },
  "optionalDependencies": {
    "@rollup/rollup-android-arm-eabi": "^4.52.5",
    "@rollup/rollup-android-arm64": "^4.52.5",
    "@rollup/rollup-darwin-arm64": "^4.52.5",
    "@rollup/rollup-darwin-x64": "^4.52.5",
    "@rollup/rollup-freebsd-arm64": "^4.52.5",
    "@rollup/rollup-freebsd-x64": "^4.52.5",
    "@rollup/rollup-linux-arm-gnueabihf": "^4.52.5",
    "@rollup/rollup-linux-arm-musleabihf": "^4.52.5",
    "@rollup/rollup-linux-arm64-gnu": "^4.52.5",
    "@rollup/rollup-linux-arm64-musl": "^4.52.5",
    "@rollup/rollup-linux-loong64-gnu": "^4.52.5",
    "@rollup/rollup-linux-ppc64-gnu": "^4.52.5",
    "@rollup/rollup-linux-riscv64-gnu": "^4.52.5",
    "@rollup/rollup-linux-riscv64-musl": "^4.52.5",
    "@rollup/rollup-linux-s390x-gnu": "^4.52.5",
    "@rollup/rollup-linux-x64-gnu": "^4.52.5",
    "@rollup/rollup-linux-x64-musl": "^4.52.5",
    "@rollup/rollup-openharmony-arm64": "^4.52.5",
    "@rollup/rollup-win32-arm64-msvc": "^4.52.5",
    "@rollup/rollup-win32-ia32-msvc": "^4.52.5",
    "@rollup/rollup-win32-x64-gnu": "^4.52.5",
    "@rollup/rollup-win32-x64-msvc": "^4.52.5"
  },
  "overrides": {
    "undici": "^5.29.0"
  },
  "engines": {
    "node": ">=20"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,md,yml,yaml}": [
      "node .github/scripts/forbidden-fields-lint.js --staged",
      "node .github/scripts/pii-regex-scan.js --staged"
    ]
  }
}
