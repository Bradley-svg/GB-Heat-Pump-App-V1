{
  "name": "greenbro-worker",
  "private": true,
  "type": "module",
  "scripts": {
    "postinstall": "node ./scripts/install-better-sqlite3.mjs",
    "prepare": "husky install",
    "build": "pnpm -r build",
    "test": "pnpm -r test",
    "lint": "pnpm -r lint",
    "typecheck": "pnpm -r typecheck && pnpm run typecheck:mobile",
    "typecheck:mobile": "pnpm --filter @greenbro/mobile run typecheck",
    "dev:web": "pnpm --filter @greenbro/dashboard-web dev",
    "dev:mobile": "pnpm --filter @greenbro/mobile start",
    "dev:worker": "pnpm --filter @greenbro/overseas-api dev",
    "install-frontend:test": "node ./scripts/test-install-frontend.mjs",
    "ops:r2:bootstrap": "node ./scripts/bootstrap-app-static.mjs",
    "publish:r2": "pnpm --filter @greenbro/overseas-api run publish:r2",
    "deploy": "pnpm --filter @greenbro/overseas-api run deploy",
    "cron:deploy": "pnpm --filter @greenbro/overseas-api run cron:deploy",
    "migrate:apply": "pnpm --filter @greenbro/overseas-api run migrate:apply",
    "migrate:apply:local": "pnpm --filter @greenbro/overseas-api run migrate:apply:local",
    "migrate:list": "pnpm --filter @greenbro/overseas-api run migrate:list",
    "seed:dev": "pnpm --filter @greenbro/overseas-api run seed:dev",
    "check:prod-shim": "node ./scripts/ensure-prod-shim-disabled.mjs --env production",
    "ops:db:sanity": "node ./scripts/db-sanity-check.mjs",
    "guard:forbidden": "node .github/scripts/forbidden-fields-lint.js",
    "guard:pii": "node .github/scripts/pii-regex-scan.js",
    "guard:all": "npm-run-all -s guard:forbidden guard:pii"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.36.0",
    "@miniflare/d1": "^2.14.4",
    "@vitest/coverage-v8": "^4.0.7",
    "better-sqlite3": "^12.4.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.38.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^6.1.1",
    "fast-glob": "^3.3.3",
    "globals": "^15.11.0",
    "husky": "^9.1.7",
    "jsdom": "^27.1.0",
    "lint-staged": "^16.2.6",
    "minimatch": "^10.1.1",
    "npm-run-all": "^4.1.5",
    "rollup": "4.52.5",
    "tar": "6.2.0",
    "ts-morph": "^23.0.0",
    "tsx": "^4.7.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2",
    "vitest": "^4.0.7",
    "wrangler": "^4.45.4"
  },
  "dependencies": {
    "itty-router": "^5.0.1",
    "jose": "^5.9.3",
    "zod": "^3.23.8"
  },
  "optionalDependencies": {
    "@rollup/rollup-android-arm-eabi": "^4.52.5",
    "@rollup/rollup-android-arm64": "^4.52.5",
    "@rollup/rollup-darwin-arm64": "^4.52.5",
    "@rollup/rollup-darwin-x64": "^4.52.5",
    "@rollup/rollup-freebsd-arm64": "^4.52.5",
    "@rollup/rollup-freebsd-x64": "^4.52.5",
    "@rollup/rollup-linux-arm-gnueabihf": "^4.52.5",
    "@rollup/rollup-linux-arm-musleabihf": "^4.52.5",
    "@rollup/rollup-linux-arm64-gnu": "^4.52.5",
    "@rollup/rollup-linux-arm64-musl": "^4.52.5",
    "@rollup/rollup-linux-loong64-gnu": "^4.52.5",
    "@rollup/rollup-linux-ppc64-gnu": "^4.52.5",
    "@rollup/rollup-linux-riscv64-gnu": "^4.52.5",
    "@rollup/rollup-linux-riscv64-musl": "^4.52.5",
    "@rollup/rollup-linux-s390x-gnu": "^4.52.5",
    "@rollup/rollup-linux-x64-gnu": "^4.52.5",
    "@rollup/rollup-linux-x64-musl": "^4.52.5",
    "@rollup/rollup-openharmony-arm64": "^4.52.5",
    "@rollup/rollup-win32-arm64-msvc": "^4.52.5",
    "@rollup/rollup-win32-ia32-msvc": "^4.52.5",
    "@rollup/rollup-win32-x64-gnu": "^4.52.5",
    "@rollup/rollup-win32-x64-msvc": "^4.52.5"
  },
  "overrides": {
    "undici": "^5.29.0"
  },
  "engines": {
    "node": ">=20"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,md,yml,yaml}": [
      "node .github/scripts/forbidden-fields-lint.js --staged",
      "node .github/scripts/pii-regex-scan.js --staged"
    ]
  }
}
