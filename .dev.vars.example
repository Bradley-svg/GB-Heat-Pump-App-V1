# Copy this file to .dev.vars to bootstrap local secrets for wrangler dev sessions.
# Replace placeholder values with credentials from the password manager.
CURSOR_SECRET=change-me-min-16-chars
ACCESS_AUD=00000000-0000-0000-0000-000000000000
ACCESS_JWKS_URL=https://example.cloudflareaccess.com/cdn-cgi/access/certs
INGEST_ALLOWED_ORIGINS=http://127.0.0.1:8787
INGEST_RATE_LIMIT_PER_MIN=120
INGEST_SIGNATURE_TOLERANCE_SECS=300
INGEST_IP_LIMIT_PER_MIN=0
INGEST_IP_BLOCK_SECONDS=60
AUTH_IP_LIMIT_PER_MIN=30
AUTH_IP_BLOCK_SECONDS=300
# Optional hook invoked when password reset emails are issued.
PASSWORD_RESET_WEBHOOK_URL=https://hooks.example.com/password-reset
# Email verification webhooks must have their own URL + secret for rotation.
EMAIL_VERIFICATION_WEBHOOK_URL=https://hooks.example.com/email-verification
EMAIL_VERIFICATION_WEBHOOK_SECRET=dev-email-secret
# Local dev environments should explicitly declare their deployment mode.
ENVIRONMENT=development
# ASSET_SIGNING_SECRET=only-if-testing-signed-assets
# Local-only shim flags. Remove before deploying anywhere shared.
ALLOW_DEV_ACCESS_SHIM=true
DEV_ALLOW_USER={"email":"local-admin@example.com","roles":["admin"],"clientIds":["profile-west"]}
PASSWORD_RESET_WEBHOOK_SECRET=dev-reset-secret
CLIENT_EVENT_TOKEN_SECRET=local-dev-telemetry-token-secret-rotate-123456
