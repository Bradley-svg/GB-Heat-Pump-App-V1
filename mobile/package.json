    "expo-haptics": "~13.0.0",
    "expo-linking": "~6.3.1",
    "expo-secure-store": "~13.0.0",
    "react": "18.2.0",
    "react-native": "0.76.6",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.9.0",
